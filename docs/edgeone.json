{
    "name": "misaka-docs",
    "headers": [
        {
            "source": "/*",
            "headers": [
                {
                    "key": "Server",
                    "value": "Microsoft-IIS/10.0"
                }
            ]
        },
        {
            "source": "/api/weixin/*",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=3600, immutable"
                }
            ]
        }
    ],
    "installCommand": "npm i -D",
    "buildCommand": "npm run docs:build",
    "outputDirectory": "./.vitepress/dist",
    "nodeVersion": "22.17.1",
    "redirects": [
        {
            "source": "/advance/upload.html",
            "destination": "/advance/no-open/upload.html",
            "statusCode": 301
        },
        {
            "source": "/advance/no-open.html",
            "destination": "/advance/no-open/develop.html",
            "statusCode": 301
        }
    ]
}